{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "plugins": ["backbone"],
  "settings": {
    "import/resolver": {
      "babel-module": {
        "extensions": [".js", ".jsx"]
      }
    }
  },
  "globals": {
    "_": false,
    "$": false,
    "document": false,
    "window": false,
    "M": false,
    "FormData": true
  }
}
